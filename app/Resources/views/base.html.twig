<script>
    $(document).ready(function () {
        $('[data-toggle=offcanvas]').click(function () {
            $('.row-offcanvas').toggleClass('active');
        });
        $('#sidebar').css('height', $('#rigth-side').css('height'));
        $(window).resize(function () {
            $('#sidebar').css('height', $('#rigth-side').css('height'));
        });
    });
</script>
{% block javascripts %}{% endblock %}


</body>
